{
  "name": "common-js-utils",
  "version": "1.5.0",
  "description": "General utility module for JavaScript.",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "clean": "rimraf dist && mkdir dist",
    "js-standard": "standard --fix",
    "js-prettier": "prettier --write",
    "test": "jest dist --color",
    "test-clean": "npm run clean && npm run build && npm run test",
    "test-watch": "jest dist --color --watch",
    "travis-test": "npm run build && jest dist"
  },
  "pre-commit": [
    "clean",
    "js-standard",
    "build",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZettaKonsult/common-js-utils.git"
  },
  "keywords": [],
  "author": "Fredrik Palmquist, Zimon Kuhs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ZettaKonsult/common-js-utils/issues"
  },
  "homepage": "https://github.com/ZettaKonsult/common-js-utils#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-es2017-node7": "^0.5.2",
    "jest": "^20.0.4",
    "jest-cli": "^21.2.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.7.2",
    "serverless-webpack": "^3.1.2",
    "standard": "^10.0.3",
    "webpack": "^3.7.1",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "object-size": "^0.1.1"
  },
  "standard": {
    "ignore": [
      "*.spec*"
    ]
  }
}
