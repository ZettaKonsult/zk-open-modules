{
  "name": "zk-csv-parser",
  "version": "2.1.4",
  "description": "Module for parsing CSV files",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && babel src --out-dir dist",
    "clean": "rimraf dist && mkdir dist",
    "js-standard": "standard --fix",
    "js-prettier": "prettier --write",
    "test-dist": "jest dist --color",
    "test-src": "jest src --color",
    "test-clean": "npm run clean && npm run build && npm run test",
    "test-watch": "jest dist --color --watch",
    "travis-test": "npm run build && jest dist"
  },
  "pre-commit": [
    "js-standard",
    "test-dist",
    "test-src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZettaKonsult/csv-parser.git"
  },
  "keywords": [],
  "author": "Zimon Kuhs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ZettaKonsult/csv-parser/issues"
  },
  "homepage": "https://github.com/ZettaKonsult/csv-parser#readme",
  "dependencies": {
    "common-js-utils": "^1.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-es2017-node7": "^0.5.2",
    "jest": "^20.0.4",
    "jest-cli": "^21.2.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.5.3",
    "rimraf": "^2.6.2",
    "standard": "^10.0.3"
  },
  "standard": {
    "ignore": [
      "*.spec*"
    ]
  }
}
