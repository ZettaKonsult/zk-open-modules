{
  "name": "zk-aws-users",
  "version": "0.4.0",
  "main": "dist/index.js",
  "author": "Zimon Kuhs <zmk.zk.dev@gmail.com>",
  "scripts": {
    "start": "node src/index",
    "test": "jest src --color",
    "test:watch": "yarn test --watch",
    "build": "babel src -d dist",
    "build:watch": "babel src -w -d dist",
    "lint": "eslint src/**/*.js",
    "travis:test": "jest src",
    "prepublish": "yarn run build",
    "postpublish": "rm -rf /dist",
    "precommit": "lint-staged && yarn run test",
    "update": "yarn test && yarn build"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-runtime": "^6.26.0",
    "eslint": "^4.12.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.3.1",
    "flow-bin": "^0.61.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "lint-staged": "^6.0.0",
    "prettier": "^1.8.2"
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --trailing-comma es5 --single-quote --no-semi --write",
      "yarn run lint",
      "git add"
    ],
    "*.json": ["prettier --write", "git add"],
    "yarn.lock": ["git rm --cached"]
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^1.28.0",
    "aws-sdk": "^2.166.0"
  }
}
